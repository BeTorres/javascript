{
  "name": "exercism-javascript",
  "version": "0.0.0",
  "description": "Exercism exercises in Javascript.",
  "author": "Katrina Owen",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/exercism/javascript"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jest": "^24.5.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "package.json"
    ]
  },
  "scripts": {
    "test": "jest --no-cache ./*",
    "watch": "jest --no-cache --watch ./*",
    "lint": "eslint .",
    "lint-test": "npm run lint && npm run test",

    "prelint:track": "node ./bin/scripts/unsync-config",
    "lint:track": "npm run lint",
    "postlint:track": "npm run sync:track",
    "test:exercise": "node ./bin/scripts/test-exercise",
    "test:track": "node ./bin/scripts/test-track",
    "test:ci": "npm run test:track -- --ci && npm run lint:track",
    "sync:track": "node ./bin/scripts/sync-config",
    "sync:exercise": "node ./bin/scripts/sync-config",
    "print:track": "node ./bin/scripts/print-config-tree"
  },
  "license": "MIT",
  "dependencies": {}
}
